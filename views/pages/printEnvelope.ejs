	<script>

		function GetURLParameter(sParam){
			var sPageURL = window.location.search.substring(1);
			var sURLVariables = sPageURL.split('&');
			for (var i = 0; i < sURLVariables.length; i++){
				var sParameterName = sURLVariables[i].split('=');
        		if (sParameterName[0] == sParam) return sParameterName[1];
			}
		}
		// function getParams(sParam){
		// 	var sPageURL = window.location.search.substring(1);
		// 	var sURLVariables = sPageURL.split('&');
		// 	for (var i = 0; i < sURLVariables.length; i++){
		// 	if (sParameterName[0] == sParam) return sParameterName[1];
		// }

		$(document).ready(function(){
			var devotee_id = GetURLParameter('devotee_id');
			var donation_id = GetURLParameter('donation_id');
			// var devotee_id = getParams('devotee_id');
			// var donation_id = getParams('donation_id');
			var tempdates1;
			$.ajax({
				url : '/donation/getReceipt',
				type : 'POST',
				headers : { "x-access-token" : localStorage.getItem('token') },
				data : { devotee_id : devotee_id, donation_id : donation_id },
				success : function(response){
					//receipt_data
					if(response.success == 1 & response.failure == 0){

						$('.receipt_no').html('E000'+donation_id);
						tempdates1 = moment(response.receipt_data[0].donation_date).format('DD-MM-YYYY');
						$('.receipt_date').html(tempdates1);
						$('.devotee_id').html(response.receipt_data[0].devotee_id);
						$('.devotee_name').html(response.receipt_data[0].title + ' '+response.receipt_data[0].fName + ' ' + response.receipt_data[0].lName);
						$('.devotee_address').html(response.receipt_data[0].address + ', PIN : ' + response.receipt_data[0].pincode);
						$('.ammount_in_words').html(response.receipt_data[0].donation_amount_in_words);
						if(response.receipt_data[0].pan_no != 'NA')
							$('.devotee_pan_no').html(' with PAN : ' + response.receipt_data[0].pan_no);
						$('.payment_type').html(response.receipt_data[0].donation_payment_type);
						if(response.receipt_data[0].donation_payment_type != 'CASH')
							$('.payment_type_details').html(' with ' + response.receipt_data[0].donation_payment_details + ' : ' + response.receipt_data[0].donation_payment_details_value );
						$('.donation_purpose').html(response.receipt_data[0].donation_purpose);
						$('.amount_in_figure').html(response.receipt_data[0].donation_amount);
						var trans_date_formatted = moment(response.receipt_data[0].trans_date).format('DD-MM-YYYY');
						$('.trans_date').html(' dated : ' + trans_date_formatted);
						//console.log(localStorage.getItem('loggedInName'));received_by
						$('.received_by_seal').html(response.receipt_data[0].received_by);
					}
				},
				error : function(){

				}
			});
		});

		
		
