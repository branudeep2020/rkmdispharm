                            //tbody += "<button type='button' data-toggle='modal' data-target='#view' data-uid='"+response.bookings[key].guest_id+"' class='view btn btn-primary btn-sm'><span class='glyphicon glyphicon-eye-open'></span></button>";
                        }
                    },
                    error: function(err){
                        console.log(err);
                    }
                });
