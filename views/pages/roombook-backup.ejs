                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-4" for="country">Country:</label>
                                    <div class="col-sm-8">
                                        <textarea name="country" id="country" style="resize: none;" class="form-control"></textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-4" for="persons">No Of Persons:</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="persons" placeholder="Enter No of Persons" name="persons">
                                    </div>
                                </div>
                                <input type="hidden" class="form-control" id="nodays" name="nodays">
                                <input type="hidden" class="form-control" id="room_id" name="room_id">
                                <div class="form-group" style="margin-left: 65px;">
                                    <button type="button" class="btn btn-primary" id="confirmBooking">Confirm Booking</button>
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                </div>
                            </form>
